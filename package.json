    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "firebase": "^10.11.1",
    "lucide-vue-next": "^0.376.0",
    "pinia": "^2.1.7",
    "radix-vue": "^1.7.3",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "vee-validate": "^4.12.6",
